{
    "manifest_version": 3,
    "name": "Random Leetcode",
    "description": "Use your leetcode lists to get a random LC problem",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-192.png",
        "128": "images/icon-512.png"
      }
    },
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-192.png",
      "128": "images/icon-512.png"
    },
    "host_permissions": [
      "*://leetcode.com/"
    ],
    "permissions": [
      "cookies"
    ]
  }
  